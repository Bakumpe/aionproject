{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "PropertyName": {
      "type": "string"
    },
    "PropertyCategory": {
      "type": "enumeration",
      "enum": [
        "Home:",
        "Apartment",
        "Stand Alone",
        "Villa",
        "Studio House",
        "Arcade Small Room",
        "ECEG",
        "Flat Building",
        "Rental Space"
      ]
    },
    "NumberOfBedRooms": {
      "type": "integer"
    },
    "PriceTag": {
      "type": "biginteger"
    },
    "NumberOfUnitsForNeighbors": {
      "type": "biginteger"
    },
    "Amenities": {
      "type": "text"
    },
    "photo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Location": {
      "type": "string"
    },
    "NumberOfBathrooms": {
      "type": "integer"
    },
    "Description": {
      "type": "text"
    },
    "Garage": {
      "type": "integer"
    },
    "HouseSize": {
      "type": "biginteger"
    },
    "LandSize": {
      "type": "biginteger"
    },
    "YearBuilt": {
      "type": "date"
    },
    "PhotoUrl": {
      "type": "json"
    },
    "PropertyOwner": {
      "type": "string"
    },
    "StatusCode": {
      "type": "enumeration",
      "enum": [
        "Ex:",
        "For Rent Monthly",
        "For Sale",
        "Easy Come"
      ]
    }
  }
}
